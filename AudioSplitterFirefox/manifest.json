{
    "manifest_version": 2,
    "name": "Audio Splitter",
    "version": "1.0",
    "description": "Route tab sound to left or right audio channel.",
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "128": "icon.png"
    },
    "web_accessible_resources": [
      "content.js"
    ]
  }